### -- PERSONAL/MAIN ACCOUNT --

Host github.com
  Hostname github.com
  User {{ .github_username }}
  IdentityFile ~/.ssh/id_ed25519

### -- WORK PERSONAL/ACCOUNT --

Host homelab
  User justin
  HostName homelab.local

Host nas
  User justinwme
  HostName nas.local
  SetEnv TERM=xterm-256color

Host mbp
  User justinwilliams
  HostName mbp.local

Host studio
  User justinwilliams
  HostName studio.local

Host m1server
  User justinwilliams
  HostName m1server.local

Host udm
  User root
  HostName 10.0.1.1
  SetEnv TERM=xterm-256color

Host *
  AddKeysToAgent yes
  ServerAliveInterval 120
  ServerAliveCountMax 30
  ForwardAgent yes

{{- if eq .chezmoi.os "darwin" }}
Include ~/.orbstack/ssh/config
{{ end }}

